"http://www.opensymphony.com/xwork/xwork-validator-1.0.3.dtd">

<validators>
	<field name="objectIdPrefix">
		<field-validator type="regex">
			<param name="fieldName">objectIdPrefix</param>
			<param name="expression">(\w|_)+</param>
			<message key="errors.objectIdPrefix" />
		</field-validator>
	</field>
</validators> 
	